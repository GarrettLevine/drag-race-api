{
  "name": "drag-race-api",
  "version": "1.0.0",
  "description": "REST API for RuPaul's Drag Race",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build-react": "webpack-dev-server",
    "serve": "NODE_PATH=`pwd`/lib nodemon server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run build-react\""
  },
  "author": "Kevan Hannah & Garrett Levine",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarrettLevine/drag-race-api.git"
  },
  "bugs": {
    "url": "https://github.com/GarrettLevine/drag-race-api/issues"
  },
  "homepage": "https://github.com/GarrettLevine/drag-race-api#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "express-rate-limit": "^2.8.0",
    "mongoose": "^4.9.5",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sequelize": "^3.30.4"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  }
}
